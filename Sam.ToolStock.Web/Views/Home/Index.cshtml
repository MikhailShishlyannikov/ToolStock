@{
    ViewBag.Title = "Home Page";
}

@if (!Request.IsAuthenticated)
{
    <h1>Entry</h1>
    <p class="lead text-center">Please log in or register</p>
    <p class="lead text-center">
        @Html.ActionLink("Log In", "Login", "Account", null, new { @class = "btn btn-primary btn-lg" })
        @Html.ActionLink("Register", "Register", "Account", null, new { @class = "btn btn-primary btn-lg" })
    </p>
}

